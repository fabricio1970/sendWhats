unit SendWhats.Model.TextMessage;

interface

uses
  Bcl.Json.Attributes;

type
  TMessageModel = class
  type
  private
    [JsonPropertyAttribute('phone')]
    FPhone: String;
    [JsonPropertyAttribute('message')]
    FMessage_: String;
    [JsonPropertyAttribute('isGroup')]
    FIsGroup: Boolean;

    procedure SetIsGroup(const Value: Boolean);
    procedure SetMessage_(const Value: String);
    procedure SetPhone(const Value: String);
  public
    property Phone: String read FPhone write SetPhone;
    property Message_: String read FMessage_ write SetMessage_;
    property IsGroup: Boolean read FIsGroup write SetIsGroup;
  end;

implementation

{ TMessageModel }

procedure TMessageModel.SetPhone(const Value: String);
begin
  FPhone := Value;
end;

procedure TMessageModel.SetMessage_(const Value: String);
begin
  FMessage_ := Value;
end;

procedure TMessageModel.SetIsGroup(const Value: Boolean);
begin
  FIsGroup := Value;
end;

end.
